		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.249.15.196:3306/mlw_base?useUnicode=true&amp;charsetEncoding=UTF-8"
			userId="13mlw" password="mlwhappy543">
		</jdbcConnection>

		<javaTypeResolver>
